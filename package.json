{
  "name": "transfer-now",
  "version": "1.0.0",
  "description": "Share your files with people using just the terminal",
  "bin": {
    "transfer-now": "bin/cli.js"
  },
  "files": [
    "bin",
    "lib",
    "README.md"
  ],
  "engines": {
    "node": ">=14"
  },
  "type": "commonjs",
  "scripts": {
    "release": "np",
    "presls:deploy": "npm run build",
    "sls:deploy": "serverless deploy --stage=prod",
    "sls:info": "serverless info --stage=prod",
    "sls:remove": "serverless remove --stage=prod",
    "sls:start": "DEBUG=true sls offline --verbose",
    "build": "rm -rf lib && tsc -p tsconfig.cli.json",
    "cli": "npm run build && API_URL=http://localhost:3000/dev node ./bin/cli",
    "cli:prod": "npm run build && node ./bin/cli",
    "prebuild": "./node_modules/.bin/eslint --ext .js,.ts src",
    "lint": "./node_modules/.bin/eslint --ext .js,.ts src --fix"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.502.0",
    "@aws-sdk/client-s3": "^3.502.0",
    "@aws-sdk/lib-dynamodb": "^3.502.0",
    "@aws-sdk/s3-presigned-post": "^3.502.0",
    "@aws-sdk/s3-request-presigner": "^3.502.0",
    "cli-table": "^0.3.11",
    "form-data": "^4.0.0",
    "meow": "^9.0.0",
    "mime": "^3.0.0",
    "node-fetch": "^2.6.7",
    "node-machine-id": "^1.1.12",
    "ora": "^5.0.0",
    "unique-names-generator": "^4.7.1",
    "update-notifier": "^5.0.0"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.4",
    "@types/form-data": "^2.5.0",
    "@types/mime": "^3.0.1",
    "@types/node": "^14.0.0",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.3",
    "serverless-plugin-typescript": "^2.1.5",
    "typescript": "^4.7.4"
  },
  "keywords": [
    "cli",
    "transfer",
    "file-sharing",
    "terminal"
  ],
  "author": "Andrea Sonny <andreasonny83@gmail.com>",
  "license": "MIT"
}
