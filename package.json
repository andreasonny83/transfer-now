{
  "name": "transfer-now",
  "version": "0.3.1",
  "description": "Share your files with people using just the terminal",
  "bin": {
    "transfer-now": "bin/cli.js"
  },
  "files": [
    "bin",
    "lib",
    "README.md"
  ],
  "engines": {
    "node": ">=14"
  },
  "type": "commonjs",
  "scripts": {
    "release": "np",
    "presls:deploy": "npm run build",
    "sls:deploy": "serverless --stage=prod deploy",
    "sls:info": "serverless --stage=prod info",
    "sls:remove": "serverless --stage=prod remove",
    "sls:start": "sls offline --verbose --printOutput",
    "build": "rm -rf lib && tsc -p tsconfig.cli.json",
    "cli": "npm run build && API_URL=http://localhost:3000 node ./bin/cli",
    "cli:prod": "npm run build && node ./bin/cli",
    "prebuild": "./node_modules/.bin/eslint --ext .js,.ts src",
    "lint": "./node_modules/.bin/eslint --ext .js,.ts src --fix"
  },
  "dependencies": {
    "aws-sdk": "^2.1194.0",
    "form-data": "^4.0.0",
    "meow": "^9.0.0",
    "mime": "^3.0.0",
    "node-fetch": "^2.6.7",
    "ora": "^5.0.0",
    "unique-names-generator": "^4.7.1",
    "update-notifier": "^5.0.0"
  },
  "devDependencies": {
    "@types/form-data": "^2.5.0",
    "@types/mime": "^3.0.1",
    "@types/node": "^14.0.0",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "serverless": "^3.21.0",
    "serverless-offline": "^9.2.0",
    "serverless-plugin-typescript": "^2.1.2",
    "typescript": "^4.7.4"
  },
  "keywords": [
    "cli",
    "transfer",
    "file-sharing",
    "terminal"
  ],
  "author": "Andrea Sonny <andreasonny83@gmail.com>",
  "license": "MIT"
}
