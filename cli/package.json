{
  "name": "transfer-now",
  "version": "1.3.1",
  "description": "Share your files with people using just the terminal",
  "bin": {
    "transfer-now": "bin/cli.js"
  },
  "files": [
    "bin",
    "lib",
    "README.md"
  ],
  "engines": {
    "node": ">=14"
  },
  "type": "commonjs",
  "scripts": {
    "build": "rm -rf lib && tsc -p tsconfig.cli.json",
    "cli": "npm run build && API_URL=http://localhost:3000/dev node ./bin/cli",
    "cli:prod": "npm run build && node ./bin/cli",
    "prebuild": "./node_modules/.bin/eslint --ext .js,.ts src",
    "test": "./node_modules/.bin/eslint --ext .js,.ts src --fix"
  },
  "dependencies": {
    "cli-table": "^0.3.11",
    "form-data": "^4.0.0",
    "meow": "^9.0.0",
    "mime": "^3.0.0",
    "node-fetch": "^2.6.7",
    "node-machine-id": "^1.1.12",
    "ora": "^5.0.0",
    "unique-names-generator": "^4.7.1",
    "update-notifier": "^7.0.0"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.4",
    "@types/form-data": "^2.5.0",
    "@types/mime": "^3.0.1",
    "@types/node": "^14.0.0",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "keywords": [
    "cli",
    "transfer",
    "file-sharing",
    "terminal"
  ],
  "author": "Andrea Sonny <andreasonny83@gmail.com>",
  "license": "MIT"
}
